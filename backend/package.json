{
  "name": "flipy-backend",
  "version": "1.0.0",
  "description": "Backend API para FLIPY - plataforma de delivery con sistema de pujas",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "echo 'Build completed'",
    "test": "jest",
    "lint": "eslint .",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:seed": "node prisma/seed.js",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "db:init": "prisma migrate dev --name init",
    "db:status": "prisma migrate status"
  },
  "keywords": [
    "delivery",
    "pujas",
    "api",
    "express",
    "prisma"
  ],
  "author": "FLIPY Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.7.1",
    "@react-google-maps/api": "^2.20.7",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "prisma": "^5.7.1",
    "socket.io": "^4.7.4"
  },
  "devDependencies": {
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  }
}
